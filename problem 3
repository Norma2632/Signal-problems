clc;
clear all;
close all;
t=[1:0.01:10];
x1=t.^2;
subplot(2,2,1);
plot(t,x1);
grid;
xlabel('Time');
ylabel('Amplitude');
title('x1=t^2');


x2=t+4;
subplot(2,2,2);
plot(t,x2);
grid;
xlabel('Time');
ylabel('Amplitude');
title('x1=t+4');

x3=t.^2+3*t;
subplot(2,2,3);
plot(t,x3);
grid;
xlabel('Time');
ylabel('Amplitude');
title('x1=t^2+3t');

x4=t.^2+7;
subplot(2,2,4);
plot(t,x4);
grid;
xlabel('Time');
ylabel('Amplitude');
title('x1=t^2+7');




